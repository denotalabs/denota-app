{
  "name": "cheq",
  "version": "0.1.0",
  "description": "Cheq Protocol Demo",
  "author": "Cheq Labs",
  "homepage": ".",
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@chakra-ui/icons": "^2.0.9",
    "@chakra-ui/react": "^2.2.9",
    "@coinbase/wallet-sdk": "3.0.1",
    "@graphprotocol/graph-cli": "^0.22.1",
    "@graphprotocol/graph-ts": "^0.22.1",
    "@openzeppelin/subgraphs": "^0.1.8-5",
    "@types/node": "^18.7.13",
    "@walletconnect/web3-provider": "1.7.3",
    "dotenv": "8.2.0",
    "eslint-plugin-import": "^2.26.0",
    "ethers": "^5.7.0",
    "formik": "^2.2.9",
    "graphql": "^16.6.0",
    "husky": "^8.0.1",
    "next": "^12.2.5",
    "prettier": "^2.7.1",
    "prettier-plugin-solidity": "^1.0.0-beta.24",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "solhint": "^3.3.7",
    "solhint-plugin-prettier": "^0.0.5",
    "typescript": "^4.7.4",
    "walletlink": "2.5.0",
    "web3modal": "1.9.5"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "solhint": "./node_modules/.bin/solhint -f table **/*.sol",
    "prettier:solidity": "./node_modules/.bin/prettier --write **/*.sol",
    "prettier:ts": "./node_modules/.bin/prettier --write **/*.tsx",
    "prepare": "husky install",
    "clean-graph-node": "rm -rf src/graph/graph-node/data/",
    "run-graph-node": "cd src/graph/graph-node && docker-compose up",
    "graph-build": "cd src/graph/subgraph && graph build",
    "create-local": "graph create --node http://localhost:8020/ CheqRegistrar/CheqRegistrar",
    "graph-ship-local": "cd src/graph/subgraph && npm run codegen && npm run deploy-local",
    "remove-graph-node": "cd src/graph/graph-node && docker-compose down",
    "remove-local": "graph remove --node http://localhost:8020/ CheqRegistrar/CheqRegistrar",
    "deploy-local": "cd src/graph/subgraph && graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 CheqRegistrar/CheqRegistrar",
    "codegen": "cd src/graph/subgraph && graph codegen",
    "graph-prepare": "mustache src/graph/subgraph/src/subgraph.template.yaml > src/graph/subgraph/subgraph.yaml",
    "graph-create-remote": "graph create --node ${GQL_HOST}:8020/ CheqRegistrar/CheqRegistrar",
    "graph-ship-remote": "cd src/graph/subgraph && npm run codegen && npm run graph-deploy-remote",
    "graph-remove-node-remote": "cd src/graph/graph-node && docker-compose down",
    "graph-remove-remote": "graph remove --node ${GQL_HOST}:8020/ CheqRegistrar/CheqRegistrar",
    "graph-deploy-remote": "cd src/graph/subgraph && graph deploy --node ${GQL_HOST}:8020/ --ipfs ${GQL_HOST}:5001 CheqRegistrar/CheqRegistrar"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.19.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "hardhat": "^2.11.2",
    "mustache": "^3.2.1"
  }
}
